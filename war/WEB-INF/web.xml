<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>QuestBook</servlet-name>
		<servlet-class>ru.nol.qbook.QuestBookServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>QuestBook</servlet-name>
		<url-pattern>/questbook</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>XMPPChat</servlet-name>
		<servlet-class>ru.nol.qbook.xmpp.ChatServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>XMPPChat</servlet-name>
		<url-pattern>/_ah/xmpp/message/chat/</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>BadService</servlet-name>
		<servlet-class>ru.nol.qbook.serv.BadService</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>GoodService</servlet-name>
		<servlet-class>ru.nol.qbook.serv.GoodService</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GoodService</servlet-name>
		<url-pattern>/serv/good</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>BadService</servlet-name>
		<url-pattern>/serv/bad</url-pattern>
	</servlet-mapping>
</web-app>
